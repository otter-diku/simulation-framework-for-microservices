{
  "id": "workload-id",
  "generators": [
    {
      "id": "productId",
      "type": "unsignedInt",
      "distribution": "uniform",
      "min": 1,
      "max": 1000
    },
    {
      "id": "productIdPriceUpdate",
      "type": "constant",
      "constant": 2
    },
    {
      "id": "userId",
      "type": "constant",
      "constant": "1000-00000-00000000"
    }    
  ],
  "transactions": [
    {
      "transactionReferenceId": "add-catalog-item",
      "count": 100,
      "data": [
        {
          "name":  "product-id",
          "generatorReferenceId": "productId"
        }
      ]
    },
    {
      "transactionReferenceId": "update-catalog-item-price",
      "count": 50,
      "data": [
        {
          "name":  "product-id",
          "generatorReferenceId": "productIdPriceUpdate"
        }
      ]
    },
    {
      "transactionReferenceId": "checkout-basket",
      "count": 10,
      "data": [
        {
          "name":  "user-id",
          "generatorReferenceId": "userId"
        }
      ]
    }
  ]
}
