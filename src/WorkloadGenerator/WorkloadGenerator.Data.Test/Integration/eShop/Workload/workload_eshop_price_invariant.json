{
  "templateId": "workload-eshop-price-invariant",
  "maxConcurrentTransactions": 10,
  "generators": [
    {
      "id": "productId",
      "type": "unsignedInt",
      "distribution": "uniform",
      "min": 1,
      "max": 10
    },
    {
      "id": "sleepSeconds",
      "type": "unsignedInt",
      "distribution": "uniform",
      "min": 1,
      "max": 5
    },
    {
      "id": "userId",
      "type": "constant",
      "constant": "10000000-0000-0000-0000-000000000000"
    }
  ],
  "transactions": [
    {
      "id": "replenish-stock",
      "transactionReferenceId": "replenish-stock",
      "count": 10,
      "data": [
        {
          "name":  "product-id",
          "generatorReferenceId": "productId"
        }
      ]
    },
    {
      "id": "price-update-1",
      "transactionReferenceId": "update-catalog-item-price",
      "count": 200,
      "data": [
        {
          "name":  "product-id",
          "generatorReferenceId": "productId"
        }
      ]
    },
    {
      "id": "checkout-3-items",
      "transactionReferenceId": "checkout-3-items",
      "count": 200,
      "data": [
        {
          "name":  "user-id",
          "generatorReferenceId": "userId"
        },
        {
          "name":  "product-id-1",
          "generatorReferenceId": "productId"
        },
        {
          "name":  "product-id-2",
          "generatorReferenceId": "productId"
        },
        {
          "name":  "product-id-3",
          "generatorReferenceId": "productId"
        }
      ]
    }    
  ]
}
